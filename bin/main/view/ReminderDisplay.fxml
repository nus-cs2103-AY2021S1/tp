<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minWidth="32.0" styleClass="content-box" xmlns="http://javafx.com/javafx/11.0.0"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" styleClass="content-box-title">
            <children>
                <Label text="Reminder" />
            </children>
        </HBox>
        <FlowPane fx:id="reminderPane" alignment="CENTER">
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="8.0" left="24.0" right="24.0" top="8.0" />
            </padding>
        </FlowPane>
    </children>
</VBox>
