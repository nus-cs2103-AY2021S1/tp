@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Class HiddenOutside #FFFFFF

Package Model <<Rectangle>> {
Interface ReadOnlyAddressBook <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class AddressBook
Class VersionedAddressBook
Class ReadOnlyAddressBook
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs
Class CurrentPath
Class File

Package Tag {
Class Tag
Class TagName
Class FileAddress
Class Label
}

HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook
VersionedAddressBook --|> AddressBook

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" VersionedAddressBook
ModelManager o-left-> "1" UserPrefs
ModelManager o-right-> "1" CurrentPath
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniqueTagList
VersionedAddressBook *--> "*" ReadOnlyAddressBook
UniqueTagList o--> "*" Tag
Tag *--> "1" TagName
Tag *--> "1" FileAddress
Tag *--> "*" Label

ModelManager -->"1" Tag : filtered list
ModelManager -->"1" File : filtered list
@enduml
