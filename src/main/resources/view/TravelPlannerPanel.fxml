<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.ListView?>

<VBox fx:id="travelPlanPanel" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="5" right="5" bottom="5" left="10" />
    </padding>
    <Pane style="-fx-padding: 0 0 100 0">
        <Label fx:id="Wanderlust" styleClass="cell_big_label" text="Wanderlust" />
    </Pane>
    <Label text="Wishlist" styleClass="header_label"/>
    <Label text="Travel Plans" styleClass="header_label"/>
    <ListView fx:id="travelPlanListView" styleClass="list-view" VBox.vgrow="ALWAYS"/>
</VBox>
