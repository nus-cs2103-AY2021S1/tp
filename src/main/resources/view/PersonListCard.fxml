<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" maxWidth="+Infinity" minWidth="10.0" prefWidth="240.0" />
         <ColumnConstraints hgrow="ALWAYS" maxWidth="+Infinity" minWidth="10.0"
                            prefWidth="62.39999999999998" />
    </columnConstraints>
   <Label fx:id="id" alignment="CENTER_RIGHT" contentDisplay="RIGHT" maxHeight="1.7976931348623157E308"
          maxWidth="+Infinity" minWidth="-Infinity" style="-fx-opacity: 0.1; -fx-font-size: 68px;"
          styleClass="cell_big_label" textAlignment="RIGHT" textOverrun="CLIP" wrapText="true" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
   </Label>
    <VBox alignment="CENTER_LEFT" minHeight="105" GridPane.columnIndex="0">
      <padding>
        <Insets bottom="5" left="15" right="5" top="5" />
      </padding>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Button id="user" mnemonicParsing="false" styleClass="icon-button" stylesheets="@DarkTheme.css" text="Button">
               <HBox.margin>
                  <Insets right="3.0" top="1.0" />
               </HBox.margin>
            </Button>
        <Label fx:id="name" styleClass="cell_big_label" text="\$first" wrapText="true" />
      </HBox>
         <HBox spacing="5.0">
            <children>
               <Button id="contact" mnemonicParsing="false" styleClass="icon-button" stylesheets="@DarkTheme.css" text="Button">
                  <HBox.margin>
                     <Insets right="3.0" top="4.0" />
                  </HBox.margin>
               </Button>
            <Label fx:id="phone" styleClass="cell_small_label" text="\$phone" wrapText="true" />
            </children>
         </HBox>
         <HBox spacing="5.0">
            <children>
               <Button id="department" mnemonicParsing="false" styleClass="icon-button" stylesheets="@DarkTheme.css" text="Button">
                  <HBox.margin>
                     <Insets right="3.0" top="4.0" />
                  </HBox.margin>
               </Button>
            <Label fx:id="departmentOffice" styleClass="cell_small_label" text="\$department" wrapText="true" />
            </children>
         </HBox>
         <HBox spacing="5.0">
            <children>
               <Button id="email" mnemonicParsing="false" styleClass="icon-button" stylesheets="@DarkTheme.css" text="Button">
                  <HBox.margin>
                     <Insets right="3.0" top="4.0" />
                  </HBox.margin>
               </Button>
            <Label fx:id="email" styleClass="cell_small_label" text="\$email" wrapText="true" />
            </children>
         </HBox>
      <Label fx:id="remark" styleClass="cell_small_label" text="\$remark" wrapText="true">
            <padding>
               <Insets bottom="5.0" top="5.0" />
            </padding>
         </Label>
      <FlowPane fx:id="tags">
            <VBox.margin>
               <Insets />
            </VBox.margin>
            <padding>
               <Insets bottom="5.0" />
            </padding>
         </FlowPane>
    </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
  </GridPane>
</HBox>
