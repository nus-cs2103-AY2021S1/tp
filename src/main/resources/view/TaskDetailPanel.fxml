<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <VBox style="-fx-background-color: #e6f2ff; -fx-background-radius: 25;" prefHeight="600" styleClass="scheduleUiPanel">
    <StackPane fx:id="commandBoxPlaceholder" styleClass="pane-without-border" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="5" left="10" right="10" top="5" />
        </padding>
    </StackPane>

    <StackPane fx:id="taskDetailCardPlaceholder" minHeight="100" styleClass="pane-without-border" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="5" left="10" right="10" top="5" />
        </padding>
    </StackPane>

    <VBox fx:id="taskList" minWidth="340" prefHeight="340" VBox.vgrow="ALWAYS">
        <StackPane fx:id="taskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
    </VBox>
      <padding>
         <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
      </padding>
    </VBox>
</VBox>
