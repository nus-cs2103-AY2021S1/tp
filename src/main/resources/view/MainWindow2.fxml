<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.stage.*?>

<fx:root onCloseRequest="#handleExit" title="ChopChop" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <scene>
      <Scene>
         <stylesheets>
              <URL value="@Style.css" />
         </stylesheets>
         <BorderPane prefHeight="560.0" prefWidth="880.0" styleClass="main-pane">
            <top>
               <MenuBar fx:id="menuBar" prefWidth="928.0" BorderPane.alignment="CENTER">
                 <menus>
                   <Menu mnemonicParsing="false" text="File">
                     <items>
                       <MenuItem fx:id="handleMenuExit" mnemonicParsing="false" onAction="#handleExit" text="Exit" />
                     </items>
                   </Menu>
                   <Menu mnemonicParsing="false" text="Help">
                     <items>
                       <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Docs" />
                     </items>
                   </Menu>
                 </menus>
               </MenuBar>
            </top>
            <right>
               <BorderPane prefHeight="520.0" prefWidth="304.0" BorderPane.alignment="CENTER">
                  <top>
                     <StackPane fx:id="commandBoxPlaceholder" prefHeight="256.0" prefWidth="304.0" BorderPane.alignment="CENTER" />
                  </top>
                  <bottom>
                     <StackPane fx:id="pinBoxPlaceholder" prefHeight="320.0" prefWidth="304.0" BorderPane.alignment="CENTER" />
                  </bottom>
               </BorderPane>
            </right>
            <left>
               <StackPane fx:id="displayPlaceholder" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
            </left>
         </BorderPane>
      </Scene>
   </scene>
</fx:root>
