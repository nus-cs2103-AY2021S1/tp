<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane prefHeight="105.0" prefWidth="170.0" HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <VBox fx:id="vbox" alignment="CENTER_LEFT" minHeight="105" styleClass="timeline-box" GridPane.columnIndex="0">
      <padding>
        <Insets bottom="5" left="15" right="5" top="5" />
      </padding>
      <Label fx:id="time" styleClass="timeline_datetime_label" text="time" textFill="WHITE">
            <graphic>
            <Label fx:id="date" styleClass="timeline_datetime_label" text="date" textFill="WHITE" />
            </graphic>
         </Label>
         <Label fx:id="module" styleClass="timeline_meetingname_label" text="\$module" textFill="WHITE" />
     <Label fx:id="name" styleClass="timeline_meetingname_label" text="\$name" textFill="WHITE" />
      <FlowPane fx:id="participants" styleClass="participants" />
    </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
  </GridPane>
</HBox>
