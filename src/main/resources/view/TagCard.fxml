<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER_LEFT" onMouseClicked="#showTagInfo" spacing="3.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
<padding>
  <Insets bottom="5" left="15" right="5" top="5" />
</padding>
<HBox alignment="CENTER_LEFT" spacing="5">
  <Label fx:id="id" styleClass="cell_big_label">
    <minWidth>
      <!-- Ensures that the label text is never truncated -->
      <Region fx:constant="USE_PREF_SIZE" />
    </minWidth>
         <font>
            <Font size="14.0" />
         </font>
  </Label>
  <Label fx:id="tagName" styleClass="cell_big_label" text="\$tagName">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
</HBox>
   <HBox fx:id="labels" spacing="5.0" VBox.vgrow="NEVER" />
<Label fx:id="fileAddress" styleClass="cell_small_label" text="\$fileAddress" />
</VBox>
