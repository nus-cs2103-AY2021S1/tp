<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.StackPane?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<fx:root resizable="false" title="Graph" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/bamboo_resized.png" />
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@DarkTheme.css" />
            </stylesheets>
            <StackPane fx:id="placeHolder" styleClass="pane-with-border">
                <padding>
                    <Insets top="5" right="10" bottom="5" left="10" />
                </padding>
                <PieChart fx:id="pieChart"/>
            </StackPane>
        </Scene>
    </scene>
</fx:root>
