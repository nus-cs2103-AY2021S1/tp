<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" spacing="20">
    <Label fx:id="projectCompletionHeading" styleClass="details_title" text="Overall Completion Percentage"/>
    <VBox alignment="CENTER" styleClass="stack-pane" >
        <StackPane fx:id="ocpDiagram"/>
        <Label fx:id="ocpCaption" text="\$ocpCaption" styleClass="details_subheading"/>
        <padding>
            <Insets bottom="25"/>
        </padding>
    </VBox>
</VBox>
