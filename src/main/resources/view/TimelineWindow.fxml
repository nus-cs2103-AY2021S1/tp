<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.stage.Stage?>

<fx:root maxWidth="1195.0" minWidth="1195.0" resizable="false" title="Timeline" type="javafx.stage.Stage" width="1195.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/clock.png" />
  </icons>
  <scene>
    <Scene fill="#121212">
         <HBox fx:id="timelineContainer" alignment="CENTER" prefWidth="1195.0" styleClass="timeline-window">
            <opaqueInsets>
               <Insets left="10.0" right="10.0" top="10.0" />
            </opaqueInsets>
            <padding>
               <Insets left="10.0" right="10.0" top="10.0" />
            </padding>
            <children>
               <ScrollPane fx:id="scrollPane" hbarPolicy="ALWAYS" prefWidth="1195.0" styleClass="scroll-bar" vbarPolicy="NEVER">
                  <content>
                     <HBox fx:id="container" prefHeight="410.0" prefWidth="1195.0" styleClass="timeline-window">
                        <opaqueInsets>
                           <Insets />
                        </opaqueInsets></HBox>
                  </content>
                  <HBox.margin>
                     <Insets />
                  </HBox.margin>
               </ScrollPane>
            </children>
         </HBox>
    </Scene>
  </scene>
</fx:root>
