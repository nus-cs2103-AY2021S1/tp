<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Label?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <Pane fx:id="countdownEventListPanelTitleHolder" minHeight="40.0" prefHeight="40.0" styleClass="all-tasks">
        <children>
            <Label fx:id="countdownEventListPanelTitle" alignment="CENTER" contentDisplay="CENTER"
                prefHeight="30.0" prefWidth="${countdownEventListPanelTitleHolder.width}"
                text="Upcoming Events" textAlignment="CENTER">
            </Label>
        </children>
    </Pane>
    <ListView fx:id="eventListView" VBox.vgrow="ALWAYS" />
</VBox>
