                 <StackPane fx:id="statisticsPlaceholder" VBox.vgrow="NEVER" />
             </VBox>
         </children>
      </HBox>

        <VBox.margin>
            <Insets />
        </VBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
